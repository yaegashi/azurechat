{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "value": "${AZURE_ENV_NAME=azurechat-solution}"
    },
    "authGithubId": {
      "value": "${AUTH_GITHUB_ID}"
    },
    "authGithubSecret": {
      "value": "${AUTH_GITHUB_SECRET}"
    },
    "azureAdTenantId": {
      "value": "${AZURE_AD_TENANT_ID}"
    },
    "azureAdClientId": {
      "value": "${AZURE_AD_CLIENT_ID}"
    },
    "azureAdClientSecret": {
      "value": "${AZURE_AD_CLIENT_SECRET}"
    },
    "azureAdAllowedPrincipals": {
      "value": "${AZURE_AD_ALLOWED_PRINCIPALS}"
    },
    "adminEmailAddress": {
      "value": "${ADMIN_EMAIL_ADDRESS}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    }
  }
}
